{
  "use": "TransFlow",
  "filter": "TransFlow.stat1",
  "key": {
    "ns": "业务号",
    "dim": "账户",
    "on": ["TransFlow.acctId"]
  },
  "flowId": "TransFlow.acctId + TransFlow.transTime",
      "name": "某账号过去30天最大交易金额",
      "alias": "maxNumber",
      "calc": {
        "oper": "max",
        "on": "TransFlow.transAmt"
      },
  "tw": {
    "on": "TransFlow.transTime",
    "tw": "30d"
  },
  "namespace": "xxxxx"
}
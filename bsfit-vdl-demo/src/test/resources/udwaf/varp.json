{
  "namespace": "fdl1",
  "use": "TransFlow",
  "filter": "isNotNull(TransFlow.stat)",
  "key": {
    "ns": "业务号",
    "dim": "账户",
    "on": ["TransFlow.acctId"]
  },
  "flowId": "TransFlow.acctId + TransFlow.transTime",
      "name": "某账号过去30天交易金额的方差",
      "alias": "varpNumber",
      "calc": {
        "oper": "varp",
        "on": "TransFlow.transAmt"
      },
  "tw": {
    "on": "TransFlow.transTime",
    "tw": "30d"
  }
}